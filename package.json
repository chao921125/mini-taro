{
  "name": "taro-vue3",
  "version": "1.0.0-alpha.18",
  "private": true,
  "description": "A demo showcase of taro-ui-vue3",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "NODE_ENV=production taro build --type weapp",
    "build:swan": "NODE_ENV=production taro build --type swan",
    "build:alipay": "NODE_ENV=production taro build --type alipay",
    "build:tt": "NODE_ENV=production taro build --type tt",
    "build:h5": "node ./config/h5-building-script.js && NODE_ENV=production taro build --type h5",
    "build:rn": "NODE_ENV=production taro build --type rn",
    "build:qq": "NODE_ENV=production taro build --type qq",
    "build:jd": "NODE_ENV=production taro build --type jd",
    "build:quickapp": "NODE_ENV=production taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "deploy:h5": "./h5_deploy.sh"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "b2nil",
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "@tarojs/components": "^3.6.8",
    "@tarojs/runtime": "^3.6.8",
    "@tarojs/taro": "^3.6.8",
    "taro-ui-vue3": "^1.0.0-alpha.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@tarojs/mini-runner": "^3.6.8",
    "@tarojs/webpack-runner": "^3.6.8",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "@vue/compiler-sfc": "^3.3.4",
    "babel-preset-taro": "^3.6.8",
    "eslint": "^8.44.0",
    "eslint-config-taro": "^3.6.8",
    "eslint-plugin-vue": "^9.15.1",
    "stylelint": "15.10.1",
    "typescript": "^5.1.6",
    "vue-loader": "^17.2.2",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
